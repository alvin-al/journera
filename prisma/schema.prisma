generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Package {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  description   String
  featuredImage String
  itinerary     String[]
  name          String
  originalPrice Int
  price         Int
  slug          String
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
