generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Package {
  id            String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt     DateTime @db.Date
  description   String
  featuredImage String
  itinerary     String[]
  name          String
  originalPrice Float
  price         Float
  slug          String
  updatedAt     DateTime @db.Date
}
